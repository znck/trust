{
  "name": "znck/trust",
  "type": "library",
  "description": "User roles and permissions for Laravel 5",
  "homepage": "https://github.com/znck/trust",
  "keywords": [
    "laravel",
    "role",
    "roles",
    "permissions",
    "trust",
    "authorisation",
    "gate"
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "^5.3",
    "illuminate/contracts": "^5.3",
    "illuminate/support": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5",
    "graham-campbell/testbench": "^3.2",
    "friendsofphp/php-cs-fixer": "^1.12.1",
    "phpunit/php-code-coverage": "^4.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Znck\\Trust\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Znck\\Tests\\Trust\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs": "php-cs-fixer fix"
  }
}
